group "G1Simulation-TestOrientCF-startRemoteCSepaLast" {
    cmd "testsenderG1" {
        auto_respawn = "true";
        exec = "testsender imuAcquisitionStatus imuAcquisitionImuRaw stateEstimatorOrientV1 detectorVis controllerPDPose controllerSOCPose controllerSOCOrient";
        host = "ra-Desktop01";
    }
    cmd "controllerPDOrientG1" {
        auto_respawn = "true";
        exec = "controllerPDOrient stateVariancesSim";
        host = "ra-Desktop01";
    }
    cmd "watchdogG1" {
        auto_respawn = "true";
        exec = "watchdog imuRawSim stateVariancesSim";
        host = "ra-Desktop01";
    }
    cmd "simulatorG1" {
        auto_respawn = "true";
        exec = "simulator";
        host = "ra-Desktop01";
    }
    cmd "motorCommanderG1" {
        auto_respawn = "true";
        exec = "motorCommander";
        host = "ra-Desktop01";
    }
    cmd "stateEstimatorOrientCFG1" {
        exec = "stateEstimatorOrientCF imuRawSim";
        host = "ra-Desktop01";
    }
}

